{
  "name": "broid-skype",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "ISC",
  "author": "Broid Team <opensource@broid.ai> (https://broid.ai)",
  "scripts": {
    "lint": "eslint src config",
    "start": "npm run build && NODE_ENV=production node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir dist"
  },
  "eslintConfig": {
    "extends": [
      "zavatta"
    ],
    "rules": {
      "no-sync": 0,
      "no-undef": 0,
      "id-length": 0,
      "camelcase": 0,
      "no-inline-comments": 0,
      "max-nested-callbacks": 0
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      [
        "coverage",
        {
          "only": "src/*/"
        }
      ],
      "transform-runtime"
    ]
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "botbuilder": "3.5.0-rc9",
    "broid-helpers": "^1.0.0",
    "broid-schemas": "^1.0.0",
    "express": "^4.14.0",
    "mimetype": "^0.0.8",
    "node-uuid": "^1.4.7",
    "ramda": "^0.22.1",
    "request-promise": "^4.1.1",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-coverage": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.12.2",
    "eslint-config-zavatta": "^4.2.0"
  }
}
